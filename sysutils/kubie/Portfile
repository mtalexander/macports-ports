destroot {
    set bashcomp_dir ${prefix}/share/bash-completion/completions

    xinstall -m 755 \
        ${worksrcpath}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/

    xinstall -d ${destroot}${bashcomp_dir}

    xinstall -m 0644 \
        ${worksrcpath}/completion/${name}.bash \
        ${destroot}${bashcomp_dir}/${name}
}

